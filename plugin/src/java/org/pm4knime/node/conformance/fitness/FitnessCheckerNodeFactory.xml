<?xml version="1.0" encoding="UTF-8"?>
<knimeNode icon="./Conformance-Fitness-icon.png" type="Predictor" xmlns="http://knime.org/node/v2.8" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://knime.org/node/v2.10 http://knime.org/node/v2.10.xsd">
    <name>Fitness Checker</name>
    
    <shortDescription>
        Based on the replay result, this node outputs the statistical fitness information. 
    </shortDescription>
    
    <fullDescription>
        <intro>This node is based on the result of alignment-based replayer. It outputs the statistical fitness information. 
        </intro>
        
    </fullDescription>
    
    <ports>
        <inPort index="0" name="Replay Result ">replay result</inPort>
        <!-- possibly more input ports here-->
        <outPort index="0" name="Fitness StatInfo">fitness statistical information</outPort>
        <!-- possibly more output ports here-->
    </ports>    
    <!-- to view work here to show the projection on model -->
    <views>
        <view index="0" name="Fitness Projection">Projection on the Petri Net to show the fitness result.</view>
      
    </views>
</knimeNode>
