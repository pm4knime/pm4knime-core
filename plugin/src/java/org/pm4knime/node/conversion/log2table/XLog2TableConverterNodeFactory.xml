<?xml version="1.0" encoding="UTF-8"?>
<knimeNode icon="./XLog2Table-icon.png" type="Manipulator"
	xmlns="http://knime.org/node/v2.8"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://knime.org/node/v2.10 http://knime.org/node/v2.10.xsd">
	<name>Event Log to Table</name>

	<shortDescription>
		Convert an event log into KNIME Data Table
	</shortDescription>

	<fullDescription>
		<intro>This node converts an event log into a KNIME Data Table.
		</intro>
		<!-- if we use the attributes, it means we need to have the log spec in 
			advance and serialization for it. I think I have done this, right?? Let me 
			check it -->

		<option name="Trace attribute set">Choose the trace attributes to be included in the table.
		</option>
		<option name="Event attribute set">Choose the event attributes to be included in the table.
		</option>

	</fullDescription>

	<ports>
		<inPort index="0" name="Event Log">an event log</inPort>
		<!-- possibly more input ports here -->
		<outPort index="0" name="Table">an event table</outPort>
		<!-- possibly more output ports here -->
	</ports>

</knimeNode>
