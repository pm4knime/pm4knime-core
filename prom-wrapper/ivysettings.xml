<ivysettings>
    <settings defaultResolver="default" />   
    <resolvers>
        <url name="prom" checkmodified="true">
            <ivy pattern="https://github.com/promworkbench/Releases/tree/main/Packages/[module]/[revision]/ivy.xml" />
            <artifact pattern="https://github.com/promworkbench/Releases/tree/main/Packages/[module]/[revision]/[artifact]-[revision].[ext]" />
        </url>
        <url name="prom-libs">
            <ivy pattern="https://github.com/promworkbench/Releases/tree/main/Libraries/[module]/[revision]/ivy.xml" />
            <artifact pattern="https://github.com/promworkbench/Releases/tree/main/Libraries/[module]/[revision]/[artifact]-[revision].[ext]" />
        </url>
        <ibiblio name="maven2" m2compatible="true"/>
        <chain name="default" returnFirst="true">  
            <resolver ref="prom" /> 
            <resolver ref="prom-libs" />   
            <resolver ref="maven2" />  
        </chain>  
    </resolvers>
</ivysettings>
